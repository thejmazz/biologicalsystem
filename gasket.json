{
  "get-annotations": [
    "bionode-quickgo -a --goid GO:0048312 --format tsv",
    "node src/parse-tsv.js",
    "dat import - -d GO0048312-annotations"
  ],
  "filter-proteins": [
    "dat export -d GO0048312-annotations",
    "node src/filter-proteins.js"
  ],
  "get-uniprot": [
    "dat export -d GO0048312-annotations",
    "node src/get-uniprot.js > files/0048312-proteins.json"
  ],
  "make-markdown": [
    "node src/gen-uniprot-md.js files/0048312-proteins.json > files/0048312-proteins.md"
  ]
}
