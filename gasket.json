{
  "get-annotations": [
    "bionode-quickgo -a --goid GO:1990456 --format tsv",
    "node src/parse-tsv.js",
    "dat import - -d GO1990456-annotations"
  ],
  "filter-proteins": [
    "dat export -d GO1990456-annotations",
    "node src/filter-proteins.js"
  ],
  "get-uniprot": [
    "dat export -d GO1990456-annotations",
    "node src/get-uniprot.js > files/1990456-proteins.json"
  ],
  "make-markdown": [
    "node src/gen-uniprot-md.js files/1990456-proteins.json > files/1990456-proteins.md"
  ]
}
