{
  "get-annotations": [
    "bionode-quickgo -a --goid GO:0047497 --format tsv",
    "node src/parse-tsv.js",
    "dat import - -d go-annotations"
  ],
  "filter-proteins": [
    "dat export -d go-annotations",
    "node src/filter-proteins.js"
  ],
  "get-uniprot": [
    "dat export -d go-annotations",
    "node src/get-uniprot.js > files/0047497-proteins.json"
  ],
  "make-markdown": [
    "node src/gen-uniprot-md.js files/0047497-proteins.json > files/0047497-proteins.md"
  ]
}
